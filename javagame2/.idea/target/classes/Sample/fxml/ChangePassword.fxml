<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sample.ChangePassword">
   <children>
      <Label alignment="CENTER" contentDisplay="BOTTOM" layoutX="23.0" layoutY="52.0" text="Please Enter your new password" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="newPassword" layoutX="63.0" layoutY="143.0" prefHeight="25.0" prefWidth="176.0" />
      <Button layoutX="63.0" layoutY="240.0" mnemonicParsing="false" onAction="#enterNewPassword" prefHeight="25.0" prefWidth="176.0" text="Enter" />
      <Button layoutX="63.0" layoutY="283.0" mnemonicParsing="false" onAction="#changePasswordBack" prefHeight="25.0" prefWidth="176.0" text="Back" />
   </children>
</AnchorPane>
