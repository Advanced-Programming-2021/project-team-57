<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sample.ChangeNickname">
   <children>
      <Label layoutX="128.0" layoutY="43.0" prefHeight="35.0" prefWidth="385.0" text="Please enter your new nickname:" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="newNickname" layoutX="218.0" layoutY="126.0" prefHeight="25.0" prefWidth="173.0" promptText="nickname" />
      <Button layoutX="225.0" layoutY="237.0" mnemonicParsing="false" onAction="#enterNewNickname" prefHeight="25.0" prefWidth="141.0" text="Enter" />
      <Button layoutX="225.0" layoutY="290.0" mnemonicParsing="false" onAction="#changeNicknameBack" prefHeight="25.0" prefWidth="141.0" text="Back" />
      <Label layoutX="239.0" layoutY="183.0" prefHeight="17.0" prefWidth="108.0" textFill="WHITE" />
   </children>
</AnchorPane>
